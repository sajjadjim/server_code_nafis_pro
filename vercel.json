{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",  // This should point to the api/index.js file
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",  // Match all requests
      "dest": "/api/index.js",  // Send all requests to the Express handler
      "methods": [
        "GET", "POST", "PUT", "PATCH", "DELETE", "OPTIONS"
      ]
    }
  ]
}
